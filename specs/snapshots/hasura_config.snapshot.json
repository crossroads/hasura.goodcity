            "foreign_key_constraint_on": "district_id"
          }
        }
      ],
      "select_permissions": [
        {
          "role": "user",
          "permission": {
            "columns": [
              "address_type",
              "addressable_id",
              "addressable_type",
              "building",
              "created_at",
              "district_id",
              "flat",
              "id",
              "street",
              "updated_at"
            ],
            "filter": {
              "_and": [
                {
                  "addressable_type": {
                    "_eq": "User"
                  }
                },
                {
                  "addressable_id": {
                    "_eq": "X-Hasura-User-Id"
                  }
                },
                {
                  "deleted_at": {
                    "_is_null": true
                  }
                }
              ]
            },
            "limit": 25
          }
        }
      ]
